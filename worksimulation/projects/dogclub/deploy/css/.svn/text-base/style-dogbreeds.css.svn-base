@import "reset.css";
body{
font-family:Trebuchet MS1, Trebuchet MS, sans-serif;
  font-size:13px;
  line-height:18px;
}
.container_24{
	width: 100%;
	text-align: center;
}
.ccontainer{
	display: block;
	width: 950px;
	text-align: left;
	margin: 0 auto;
	padding: 40px 0;
	height: 140px;
}
article{padding: 0; margin: 0;}
.wrapper{text-align: left;}
/*======================= DogBreed Page =================*/
.gradient{
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, rgba(236,221,200,1) 0%, rgba(243,226,199,0.22) 7%, rgba(243,226,199,0) 9%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(236,221,200,1)), color-stop(7%,rgba(243,226,199,0.22)), color-stop(9%,rgba(243,226,199,0))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, rgba(236,221,200,1) 0%,rgba(243,226,199,0.22) 7%,rgba(243,226,199,0) 9%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, rgba(236,221,200,1) 0%,rgba(243,226,199,0.22) 7%,rgba(243,226,199,0) 9%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, rgba(236,221,200,1) 0%,rgba(243,226,199,0.22) 7%,rgba(243,226,199,0) 9%); /* IE10+ */
background: linear-gradient(to bottom, rgba(236,221,200,1) 0%,rgba(243,226,199,0.22) 7%,rgba(243,226,199,0) 9%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ecddc8', endColorstr='#00f3e2c7',GradientType=0 ); /* IE6-8 */
}
#divisor{
	margin: 39px 0;
	padding: 0;
	height: 1px;
	width: 950px;
	border-top: solid #e9e9e9;
	float: left;
}

#breedTitle h3{
margin: 35px 0 30px;
padding: 0;
font-size: 24px;
line-height: 29px;
color: #454544;
font-weight: bold;
text-transform: uppercase;

}
.breedInfo, #breedDog{
	float: left;
}
.breedInfo{
	width: 380px;
	vertical-align: top;
	
}
#breedDog{
	background: url(../images/dogbreeds/2page_img1.jpg) no-repeat;
	width: 547px;
	height: 328px;
	border: 1px solid #d3d3d5;
	margin: 0 21px 0 0;
}

#breeds {
width: 950px;
margin: auto;
}



#breeds h4{
	color:#c98322;
	margin: 0 0 11px;
	padding: 0;
}

#breeds p{
	outline: 0;
	margin: 0 0 11px;
	padding: 0;
	color: #b6b6b6;
	text-align: left;
}

.colored{
	color:#c98322 !important;
	font-weight: bold;
}

#breeds strong{
	color: #000000;
}

#breedNews h4{
font-size: 24px;
line-height: 29px;
font-weight: bold;
text-transform: uppercase;
margin: 0 0 40px;
}

#breedNews h5{
	margin: 0 0 5px;
	padding: 0;
}
.topTen{
	width: 590px;
	float: left;
}
.topTen ul{

}

.topTen ul li{
	display: inline-block;
	float: left;
	margin: 0 4px 20px;

}
.topTen img:hover{
	border-color: #c98322;
}
.topTen img{
	border: 1px solid #e9e9e9;
	margin: 0 0 20px;
}

.topTen p{
	margin: 20px 0; 
}
.popular {
	float: left;
	width: 310px;
	padding: 0 0 0 50px;
}

.popular a{text-decoration: none;}
.popular a:hover p{
	color:#569017 !important; 
}

/*======================= Footer ========================*/
footer .content,.content2,.copyR{float: left; display: inline;}
footer .Container{padding: 65px;}

footer .content{
	width: 230px;	
}
footer .content, .content2{padding: 25px 0 0 0;}
footer .content2{ width: 180px;}

footer .copyR{
	width: 18%;
	background: url(../images/home/f_logo.png) no-repeat;
	height: 80px;
	padding: 45px 0 0 80px;
}

footer .copyR, .copyR a{
	color:#c2c2c2;
	text-decoration: none;}

footer .copyR a:hover{color: #000000;}
footer{
	background-color: #f0f0f0;
	border-top: 3px solid #e7e7e7;
	margin: auto;
	text-align: center;
	
}
footer p {margin: 0;padding: 0;}
footer ul{
	list-style: url(../images/home/marker1.png);
}

footer li a{
	text-decoration: none;
	color: #000000;
	
}

footer ul li a:hover{color:#569017;}

footer ul li a:visited a:active { color: #000000;}

 #social {
 	float: right;
 	overflow:hidden;
  width: 80px;
  position: relative;
  right: -40px;
  top: -50px;
 	}
 #social li{
	display: inline-block;
}
#social a{
  display:inline;
  float:left;
  width:22px;
  height:22px;
  overflow:hidden;
  
}
#social img{
	overflow: hidden;	
}
#social a:hover img{
  margin-top:-22px;
}

