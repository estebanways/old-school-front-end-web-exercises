/********************** CONTENT ************************/
#home {
    background: url(../images/home/content-bg.gif) repeat scroll 0 0 transparent;
    padding: 0 0 39px;
    position: relative;
    width: 100%;
    z-index: 1;
}

aside {
	width: 100%;
	padding: 0;
	position: relative;
	z-index: 1;
	background-color: #3ec4d9;
	background: url(../images/home/aside-bg.gif) repeat-x 0 0;
}
aside .block, #home .block{
	padding: 0;
}
aside .block .box1{
	padding-right: 13px;
}

.t3 {
	color: #FFF;
	font-size: 24px;
	line-height: 29px;
	display: block;
}
.t4 {
	color: #FFF;
	font-size: 36px;
	margin-top: -7px;
	line-height: 44px;
	margin-bottom: 15px;
	display: block;
}

.box1 {
	padding: 45px 0 57px;
	background:url(../images/home/divider2.png) repeat-y right 0;
}

aside p {
	color: #fff;
	padding-bottom: 25px;
}

.b1 {
	display: inline-block;
	background:#f8fdfd;
	color: #000;
	border-radius: 7px 0 0 7px;
	font-size: 13px;
	position: relative;
	line-height: 15px;
	padding: 9px 14px 10px 17px;
}

.b1:after {
	position: absolute;
	content: '';
	background:url(../images/home/marker2.png) no-repeat 0 0;
	width: 30px;
	height: 34px;
	display: block;
	top: 0;
	right: -30px;
}

.b1:hover {
	text-decoration: none;
	color: #fff;
	background:#4c4f4f;
}

.b1:hover:after {
	background-position: 0 bottom;
}

