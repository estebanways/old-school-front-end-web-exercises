/*****************************
*         Main Menu
*****************************/
 .mainMenu {
    margin:140px 0 0 0;
    height:74px;
    background:url('../images/home/nav_bg.gif') 50% 0 repeat;
}
.mainMenu .ulMenu {
    margin-left: 290px;
}
.mainMenu ul {
    height: 100%;
    list-style: none;
    padding: 0;
    margin:0;
}
.mainMenu ul li {
    cursor:pointer;
}
.mainMenu a {
    text-decoration: none;
    color:#ffffff;
    font:normal normal normal 20px'Oswald', sans-serif;
}
.mainMenu .first-level {
    position: relative;
    display: inline;
    /*IE6 bug*/
    float: left;
    height: 74px;
    padding:0 27px 0 25px;
    border-left:1px solid #1c1c1b;
    /*CSS3*/
    -moz-box-shadow:1px 0 1px #1c1c1b inset, -0.8px 0 1px #5b5a56;
    -webkit-box-shadow:1px 0 1px #1c1c1b inset, -0.8px 0 1px #5b5a56;
    box-shadow:1px 0 1px #1c1c1b inset, -0.8px 0 1px #5b5a56;
    behavior:url(PIE/PIE.htc);
}
.mainMenu .first-level:last-child {
    /*CSS3*/
    -moz-box-shadow:none;
    -webkit-box-shadow:none;
    box-shadow:none;
    behavior:url(PIE/PIE.htc);
}
.mainMenu .first-level a {
    display: inline-block;
    margin-top:25px;
    text-transform: uppercase;
    line-height: 26px;
    /*CSS3*/
    -moz-transition:color 0.3s ease-in;
    -webkit-transition:color 0.3s ease-in;
    transition:color 0.3s ease-in;
    behavior:url(PIE/PIE.htc);
}
.mainMenu .first-level:hover .oswald {
    color:#b22628;
}
.mainMenu .first-level .arrow-down {
    position: relative;
    display: block;
    background: url("../images/home/anchor.png") no-repeat center 0;
    height: 6px;
    margin-bottom: 14px;
    width: 100%;
}
.mainMenu .first-level .second-level {
    position: absolute;
    display: none;
    width: 148px;
    left: 0;
    padding:15px 0 27px 0;
    background-color: #303030;
    z-index: 2;
}
.mainMenu .second-level .subitems {
    position: relative;
    height: 30px;
    background: #303030 url('../images/home/sub_li.png')no-repeat 32px center;
}
.mainMenu .second-level .subitems a {
    display: inline-block;
    margin: 9px 0 0 42px;
    color:#727272;
    font:normal normal normal 1em normal Arial, Helvetica, sans-serif;
}
.mainMenu .second-level .subitems:hover .sub-a {
    color:#ffffff;
}
.mainMenu .second-level .thrid-level {
    position: absolute;
    display: none;
    padding-right: 28px;
    left: 100%;
    top:0;
    width:100%;
    background:url('../images/home/nav_bg.gif') repeat;
    z-index: 2;
    overflow: hidden;
}
.mainMenu .second-level .thrid-level li {
    display:inline;
    float: left;
}
.mainMenu .second-level .thrid-level li a {
    margin:9px 0 0 9px;
}
.mainMenu .second-level .thrid-level li:hover a {
    color:#ffffff;
}
.mainMenu .selected {
    color: #8a8989;
}
